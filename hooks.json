[
    {
      "id": "webhook",
      "execute-command": "/home/dockerComposeFiles/scripts/launcher.sh",
      "command-working-directory": "/home/dockerComposeFiles",
      "response-message": "I got the payload!",
      "response-headers":
      [
        {
          "name": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ],
      "pass-arguments-to-command":
      [
        {
          "source": "payload",
          "name": "head_commit.id"
        },
        {
          "source": "payload",
          "name": "pusher.name"
        },
        {
          "source": "payload",
          "name": "pusher.email"
        }
      ],
      "trigger-rule":
      {
        "and":
        [
          {
            "match":
            {
              "type": "payload-hmac-sha1",
              "secret": "mysecret",
              "parameter":
              {
                "source": "header",
                "name": "X-Hub-Signature"
              }
            }
          },
          {
            "match":
            {
              "type": "value",
              "value": "refs/heads/master",
              "parameter":
              {
                "source": "payload",
                "name": "ref"
              }
            }
          }
        ]
      }
    }
  ]
  